<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<TestCase xmlns="https://www.steepgraph.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="https://www.steepgraph.com ../../../../resources/xsd/TestAutomationFramework.xsd">
 
<Login username="$csv{username}" password="$csv{password}"/>
	<SwitchToDefaultContent />	
	<SetStartWindow />
	<OpenCompassApp quadrant="east" appname="Component Request Management" wait="2000" />
	<SwitchToWindow/>
	<SwitchToDefaultContent />
	<SwitchToFrame locatorType="xpath" locatorExpression="//iframe[starts-with(@id,'frame-preview-')]" id="Frame"/>	
	<ClickElement locatorType="xpath" locatorExpression="//div[@cell-id='0']"/>	
	<Wait time="1000" />
	<FindElement locatorType="xpath" locatorExpression="//div[@class='wux-controls-responsivetileview-maincontent']//div[@class='wux-controls-responsivetileview-titlebar']//div[text()='TESTAAAAAAAAAAAAAAAA']" id="check"/>
	<Action name="rightclick" refid="check"/>
	<ClickElement locatorType="xpath" locatorExpression="//div[@data-wux-menu-item-uid='0']"/>
	<ClickElement locatorType="xpath" locatorExpression="//div[@class='title-section-buttons']//span[@class='fonticon fonticon-open-down actions-menu-icon']" mode="js"/>
	<FindElement locatorType="xpath" locatorExpression="//li[@name='approve']"/>
	<CheckElement locatorType="xpath" locatorExpression="//li[@name='approve']" criteria="class: disabled"/> 
	
	
	
	<SwitchToDefaultContent/> 
	<SwitchToStartWindow />
 <Logout/>     
 </TestCase>